{
    "name": "rlt/rlt-api",
    "description": "SPb realty service API",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "Tiurula",
            "email": "tiurula2015@mail.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "RltBundle\\": "src/RltBundle"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~7.2",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-gmp": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-pgsql": "*",
        "ext-xml": "*",
        "lib-openssl": "*",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "doctrine/orm": "^2.6",
        "elastic-email/web-api-client": "^1.0",
        "friendsofsymfony/rest-bundle": "^2.1",
        "friendsofsymfony/user-bundle": "^2.0",
        "guzzlehttp/guzzle": "^6.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "jms/serializer-bundle": "^2.0",
        "lexik/jwt-authentication-bundle": "^2.2",
        "namshi/cuzzle": "^2.0",
        "nelmio/api-doc-bundle": "^2.0",
        "nelmio/cors-bundle": "^1.5",
        "opensoft/doctrine-postgres-types": "^1.3",
        "opsway/doctrine-dbal-postgresql": "^0.7",
        "oro/doctrine-extensions": "dev-master",
        "phpdocumentor/reflection-docblock": "^4.0",
        "predis/predis": "^1.1",
        "ruflin/elastica": "^6.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/symfony": "3.4.*",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.6",
        "liip/functional-test-bundle": "^1.7",
        "phpstan/phpstan-doctrine": "^0.10.0",
        "phpstan/phpstan-symfony": "^0.10",
        "phpunit/phpunit": "^7.2",
        "roave/security-advisories": "dev-master",
        "symfony/maker-bundle": "^1.5",
        "symfony/phpunit-bridge": "^4.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": [
            {
                "file": "app/config/parameters.yml"
            },
            {
                "file": "app/config/parameters_test.yml"
            }
        ],
        "branch-alias": null
    }
}
